{
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": {
            "source.fixAll": "always"
        }
    },
    "files.associations": {
        "*.css": "tailwindcss",
        "*.scss": "tailwindcss"
    },
    "prettier.bracketSpacing": false
}
