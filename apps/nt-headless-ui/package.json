        "@storybook/addon-links": "8.4.7",
        "@storybook/manager-api": "8.5.2",
        "@storybook/nextjs": "8.4.7",
        "@storybook/react": "8.4.7",
        "@storybook/theming": "8.5.2",
        "@types/node": "20",
        "@types/react": "18.2.25",
        "@types/react-dom": "18.2.25",
        "eslint-config-next": "15.1.4",
