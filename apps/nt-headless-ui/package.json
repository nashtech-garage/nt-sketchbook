        "cmdk": "1.0.4",
        "lucide-react": "0.474.0",
        "next": "15.1.4",
        "react": "18.2.0",
        "react-dom": "18.2.0",
        "storybook": "8.5.1",
        "tailwind-merge": "2.6.0",
        "tailwindcss": "3.4.17",
