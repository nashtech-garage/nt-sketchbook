{
    "name": "@nashtech-garage/headless-ui",
    "version": "0.0.12",
    "private": false,
    "type": "module",
    "scripts": {
        "build": "pnpm --filter ../nt-stylesheet build && tsc && vite build",
        "start": "next start",
        "lint": "next lint  && npx stylelint \"**/*.scss\"",
        "dev": "pnpm --filter ../nt-stylesheet build && storybook dev -p 6006",
        "build:storybook": "pnpm --filter ../nt-stylesheet build && storybook build",
        "test": "pnpm vitest",
        "typecheck": "tsc --pretty --noEmit"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/headless-ui.umd.js",
    "module": "./dist/headless-ui.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "./package.json": "./package.json",
        ".": {
            "require": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.cjs"
            },
            "default": {
                "types": "./dist/index.d.ts",
                "default": "./dist/index.js"
            }
        }
    },
    "sideEffects": false,
    "peerDependencies": {
        "react": "18.x || 19.x",
        "react-dom": "18.x || 19.x"
    },
    "dependencies": {
        "@nashtech-garage/stylesheet": "workspace:*",
        "@radix-ui/react-avatar": "catalog:",
        "@radix-ui/react-checkbox": "catalog:",
        "@radix-ui/react-collapsible": "catalog:",
        "@radix-ui/react-dialog": "catalog:",
        "@radix-ui/react-popover": "catalog:",
        "@radix-ui/react-progress": "catalog:",
        "@radix-ui/react-select": "catalog:",
        "@radix-ui/react-separator": "catalog:",
        "@radix-ui/react-slot": "catalog:",
        "@radix-ui/react-tooltip": "catalog:",
        "@tanstack/react-table": "catalog:",
        "chart.js": "catalog:",
        "class-variance-authority": "catalog:",
        "clsx": "catalog:",
        "cmdk": "catalog:",
        "lucide-react": "catalog:",
        "next": "catalog:",
        "react": "catalog:",
        "react-chartjs-2": "catalog:",
        "react-dom": "catalog:",
        "sass": "catalog:",
        "storybook": "catalog:",
        "tailwind-merge": "catalog:",
        "tailwindcss": "catalog:",
        "tailwindcss-animate": "catalog:",
        "vite": "catalog:",
        "vite-plugin-dts": "catalog:"
    },
    "devDependencies": {
        "@chromatic-com/storybook": "catalog:",
        "@eslint/eslintrc": "catalog:",
        "@storybook/addon-actions": "catalog:",
        "@storybook/addon-essentials": "catalog:",
        "@storybook/addon-interactions": "catalog:",
        "@storybook/addon-links": "catalog:",
        "@storybook/manager-api": "catalog:",
        "@storybook/nextjs": "catalog:",
        "@storybook/react": "catalog:",
        "@storybook/theming": "catalog:",
        "@testing-library/react": "catalog:",
        "@types/react": "catalog:",
        "@types/react-dom": "catalog:",
        "eslint": "catalog:",
        "eslint-config-custom-lib": "workspace:*",
        "eslint-config-next": "15.1.4",
        "jsdom": "catalog:",
        "postcss-import": "catalog:",
        "postcss-nesting": "catalog:",
        "stylelint": "catalog:",
        "stylelint-custom-lib": "workspace:*",
        "tsconfig": "workspace:*",
        "typescript": "catalog:",
        "vitest-canvas-mock": "catalog:",
        "@typescript-eslint/parser": "catalog:"
    },
    "publishConfig": {
        "registry": "https://npm.pkg.github.com"
    }
}
